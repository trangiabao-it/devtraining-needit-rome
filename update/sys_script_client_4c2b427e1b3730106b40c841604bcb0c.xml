        <script><![CDATA[function onLoad() {
    var von = 10000;
    var moiThang = 0;
    var thang = 8 * 12;
    var tiLe = 0.05;
    var total = 0;
    var loi = 0;

    for (var i = 0; i < thang; i++) {
        if (total === 0) {
            total += von;
        }
        var loiThang = (total + moiThang) * tiLe;
        console.log('lời tháng ' + i + ": " + Math.floor(loiThang));
        total += (loiThang + moiThang);
    }
    loi = total - von - moiThang * thang;
    alert('Tiền lời: ' + Math.floor(loi) + '\nTiền tổng: ' + Math.floor(total) + '\nTiền gốc: ' + (total - loi));
}]]></script>
